/* index/index.wxss */

page {
  background-color: #F5F6FA;
  height: auto;
  padding-bottom: 200rpx;
}



.banner {
  margin-top: 210rpx;
  width: 100%;
  /* display: flex; */
  justify-content: center;
}

/* 卡片式轮播 */
swiper {
  margin-top: 10rpx;
}

.slide-image {
  width: 100%;
  height: 100%;
  border-radius:9px;
  /* -webkit-border-radius: 9px; */
  /* -moz-border-radius: 9px; */
}

.zoom-out {
  transform: scale(0.94);
  transition: all 0.7s ease-out 0s;
  border-radius: 10rpx;
}

.zoom-in {
  transform: scale(1);
  transition: all 0.7s ease-in 0s;
}



.team {
  width: 690rpx;
  margin: 0 auto;
  background-color: #fff;
  border-radius: 20rpx;
  margin: 30rpx;
  padding-bottom: 20rpx;
}

.team_list {
  margin: 20rpx;
  display: flex;
  text-align-last: justify;

}

.title {
  margin-left: 20rpx;
  font-size: 36rpx;
  padding-top: 20rpx;
  color: #0F1826;
}

.team_list image {
  width: 100%;
  margin-right: 10rpx;
  height: 220rpx;
  /* border: 2rpx dashed #ccc; */
  /* display: inline; */
}

.introduct {
  width: 640rpx;
  height: 210rpx;
  display: block;
  margin: 20rpx;
  line-height: 42rpx;
  text-align: justify;
  color: rgba(110, 117, 128, 1);
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
}

.introduct_img {
  padding: 20rpx;
  margin-top: -25rpx;

}

.introduct_img image {
  width: 100%;
  height: 220rpx;
  border-radius: 20rpx;
}

.introduct_list {
  display: flex;
}

.list_1 {
  flex: 1;
  text-align: center;
}

.list_1 image {
  width: 100rpx;
  height: 100rpx;
  /* border: 2rpx dashed #ccc; */
  border-radius: 10rpx;
}

.list_1 text {
  display: block;
  height: 50rpx;
  line-height: 50rpx;
}

.list_1_text1 {
  display: block;
  overflow-wrap: break-word;
  color: rgba(110, 117, 128, 1);
  font-size: 28rpx;
  font-family: PingFangSC-Regular;
  white-space: nowrap;
}

.list_1_text2 {
  display: block;
  overflow-wrap: break-word;
  color: rgba(15, 24, 38, 1);
  font-size: 32rpx;
  font-family: PingFangSC-Regular;
  white-space: nowrap;
}


.my_bottom {
  z-index: 999;
  position: fixed;
  bottom: 0;
  width: 100%;
  border-top: 2rpx solid #EBEBEB;
  padding-top: 30rpx;
  background-color: #F5F6FA;
  padding-bottom: 50rpx;

}

.index {
  width: 120rpx;
  margin-left: 15%;
}

.index view,.my view{
  width: 100%;
  height: 50rpx;
  line-height: 50rpx;
}
.index image,.my image{
  margin-left: 23%;
}

.my {
  width: 120rpx;
  margin-right: 15%;
}

.index image,
.my image {
  width: 50rpx;
  height: 50rpx;
}




/* 自定义顶部栏高度 */
.nav {
  width: 100%;
  overflow: hidden;
  /* position: relative; */
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10;
}

.nav-title {
  width: 100%;
  height: 45px;
  line-height: 45px;
  text-align: center;
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: 10;
  font-family: PingFang-SC-Medium;
  font-size: 36rpx;
  letter-spacing: 2px;
}

.nav .back {
  width: 22px;
  height: 22px;
  position: absolute;
  bottom: 0;
  left: 0;
  padding: 10px 15px;
}

.bg-white {
  background-color: #ffffff;
}

.bg-gray {
  background-color: #f7f7f7;
}

.overflow {
  overflow: auto;
}

.hidden {
  overflow: hidden;
}

.INinputheader {
  width: 65%;
  height: 30px;
  background: rgba(235, 237, 240, 1);
  border-radius: 30px;
  font-size: 14px;
  margin-top: 7px;
  position: relative;
  float: left;
  margin-left: 12px;
}

.INsearchicon {
  position: absolute;
  left: 16rpx;
  margin-top: 6rpx;
}

.weui-input {
  height: 30px;
  line-height: 30px;
  text-align: left;
  padding-left: 30px;
  letter-spacing: 0px;
}

.INtab {
  text-align: left;
  margin-left: 12px;
  color: #fff;
}