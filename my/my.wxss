/* my/my.wxss */

.my_top {
    padding: 50rpx 0 30rpx;
    width: 100%;
    background-color: #E2F4D0;
    margin-bottom: 50rpx;
}

.touxiang {
    display: flex;
    justify-content: center;

}

.touxiang image {
    width: 140rpx;
    height: 140rpx;
    border-radius: 50%;
    border: 4rpx solid #fff;
}

.name {
    margin: 15rpx auto;
    text-align: center;
}

.my_list {
    margin: 30rpx;
    height: 50rpx;
}

.my_list image {
    margin-top: 4rpx;
    width: 44rpx;
    height: 44rpx;
}

.my_list text {
    margin-left: 30rpx;
    line-height: 50rpx;
    height: 50rpx;
    display: inline-block;
    font-size: 28rpx;
}

