.my_bottom {
    z-index: 999;
    position: fixed;
    bottom: 0;
    width: 100%;
    border-top: 2rpx solid #EBEBEB;
    padding-top: 30rpx;
    background-color: #F5F6FA;
    padding-bottom: 50rpx;
  
  }
  
  .index {
    width: 120rpx;
    margin-left: 15%;
  }
  
  .index view,.my view{
    width: 100%;
    height: 50rpx;
    line-height: 50rpx;
  }
  .index image,.my image{
    margin-left: 23%;
  }
  
  .my {
    width: 120rpx;
    margin-right: 15%;
  }
  
  .index image,
  .my image {
    width: 50rpx;
    height: 50rpx;
  }
  